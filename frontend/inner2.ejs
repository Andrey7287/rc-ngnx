<!DOCTYPE html>
<html lang="ru">

<head>
	<meta charset="UTF-8" />
	<meta name="viewport" content="initial-scale=1.0, width=device-width" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<link rel="shortcut icon" href="./favicon.ico">
</head>

<body>

	<header class="header">
		<div class="container header__container">
			<a href="/" class="site-logo">
				<!--<img src="./images/logo.png" alt="ingenix logo" />-->
				<picture>
					<source srcset="./images/logo-small.png" media="(max-width: 576px)">
					<img src="./images/logo.png" alt="ingenix logo" />
				</picture>
			</a>
			<nav class="site-nav-outer">
				<button class="c-hamburger c-hamburger--htx">
					<span>toggle menu</span>
				</button>
				<!-- Site menu data -->
				<%
					var menuItems = [
						{'О&nbsp;компании': './inner1.html'},
						{'Услуги и продукты': './inner1.html'},
						{'Новости и пресса': './inner1.html'},
						{'Медиатека': './inner1.html'},
						{'Контакты': './inner2.html' }
					]
				%>
					<!-- Site menu template -->
					<div class="site-nav-inner">
						<ul class="site-nav">
							<%	menuItems.forEach((item, idx)=>{
								for (key in item) { %>
								<li class="site-nav__item<% if(!idx){ %> act<%}%>">
									<a class="site-nav__link" href="<%=item[key] %>">
										<%=key %>

											<svg width="10" height="10" class='corner' xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve" viewBox="0 0 100 100" preserveAspectRatio="none">
												<% if( idx % 2 === 0 ) { %>
												<polygon class="corner__outer" points="0,0 0,100 100,14 100,0"/>
												<polygon class="corner__inner" points="0,0 0,60 100,0"/>
												<% } else { %>
												<polygon class="corner__outer" points="0,0 0,14 100,100 100,0"/>
												<polygon class="corner__inner" points="0,0 100,0 100,60"/>
												<% } %>
											</svg>
									</a>
									<% if(idx === 1 || idx === 2){ %>
										<ul class="dropdown">
											<li class="dropdown__item">
												<a href="">Технико-экономическая оценка</a>
											</li>
											<li class="dropdown__item">
												<a href="">Геолого-технологические решения</a>
											</li>
											<li class="dropdown__item">
												<a href="">Программное обеспечение для оценки затрат</a>
											</li>
										</ul>
										<% } %>
								</li>
								<%	}
							});	%>
						</ul>
					</div>
			</nav>
			<div class="lang-toggle">
				<svg width="10" height="10" class='corner' xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xml:space="preserve" viewBox="0 0 100 100" preserveAspectRatio="none">
					<polygon  class="corner__outer" points="0,0 0,100 100,0 "/>
					<polygon class="corner__inner" points="0,0 0,80 100,0"/>
				</svg>
				<div>
					<a href="/">home</a>
				</div>
				<div>
					<button class="lang-toggle__item act">rus</button>
					<button class="lang-toggle__item">eng</button>
				</div>
			</div>

			<div class="header-inner">
				<div class="header-inner__item site-slogan">
					Комплексный консалтинг<br />для нефтегазовой отрасли
				</div>
				<div class="header-inner__item site-search">
					<button class="site-search__toggle">search</button>
					<div class="site-search__wrapp">
						<div class="site-search__query">
							Поиск: <span> по сайту</span> <a href="f"> по нефтегазовым ресурсам</a>
						</div>
						<div class="search">
							<form action="/search/" method="get">
								<input class="search__input" type="text" name="q" placeholder="" />
								<input class="search__submit" type="submit" value="найти" />
							</form>
						</div>
					</div>
				</div>
				<div class="header-inner__item site-contacts">
					<a class="site-contacts__phone" href="tel:+74957830009">+7(495) 783 00 09</a>
					<div class="site-contacts__social">
						<a href="" class="soc soc_fb">facebook</a>
						<a href="" class="soc soc_tw">twitter</a>
						<a href="" class="soc soc_in">linkedin</a>
					</div>
				</div>
			</div>

		</div>

	</header>
	<!-- HEADER end-->

	<div class="content">

		<div class="main inner">
			<div class="container">

				<!--  breadcrumbs -->
				<div class="breadcrumbs">
					<a class="breadcrumbs__item" href="#">Главная</a>|<span class="breadcrumbs__item">О&nbsp;компании</span>
				</div>

				<h1>Контакты</h1>
				<div class="contacts">
					<div>115184, г. Москва, Руновский пер., д. 10, стр. 1 </div>
					<a class="h4" href="tel:+74957830009">+7(495) 783 00 09</a>
					<h4>Напишите нам</h4>
				</div>
				<form action="">
					<div class="contacts-form">
						<div class="contacts-form__item">
							<label for="010101">Представьтесь, пожалуйста<sup>*</sup></label>
							<input type="text" id="010101" />
						</div>
						<div class="contacts-form__item">
							<label for="020202">Электронная почта<sup>*</sup></label>
							<input type="text" id="020202" />
						</div>
						<div class="contacts-form__item">
							<label for="030303">Сообщение</label>
							<textarea name="" id="030303"></textarea>
						</div>
					</div>
				</form>


			</div>

		</div>

	</div>

